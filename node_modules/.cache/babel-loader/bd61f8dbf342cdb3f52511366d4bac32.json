/key\";\nexport default {\n  data: function data() {\n    return {\n      chapterDetails: ChapterDetails,\n      eventsData: [],\n      showLoader: true,\n      showData: false,\n      notFoundUpcomingEventFlag: false,\n      errorMsg: \"\",\n      errorAlert: false\n    };\n  },\n  created: function created() {\n    var _this = this;\n\n    fetch(\"https://cors-anywhere.herokuapp.com/https://api.meetup.com/\" + MeetupAPI.urlname + \"/events?&sign=true\").then(function (data) {\n      return data.json();\n    }).then(function (res) {\n      if (res.length > 0) {\n        _this.showLoader = false;\n        _this.showData = true;\n        _this.eventsData = res;\n      } else {\n        _this.showLoader = false;\n        _this.notFoundUpcomingEventFlag = true;\n      }\n    }).catch(function (e) {\n      _this.showLoader = false;\n      _this.errorMsg = \"Issue found with \" + e;\n      _this.errorAlert = true;\n      _this.notFoundUpcomingEventFlag = true;\n    });\n  },\n  methods: {\n    getCharString: function getCharString(data) {\n      var splitArr = data.split(\" \");\n\n      if (splitArr.length > 1) {\n        return (splitArr[0].substring(0, 1) + \"\" + splitArr[1].substring(0, 1)).toUpperCase();\n      } else {\n        return splitArr[0].substring(0, 1).toUpperCase();\n      }\n    }\n  },\n  filters: {\n    summery: function summery(val, num) {\n      return val.substring(0, num) + \"...\";\n    }\n  }\n};",{"version":3,"sources":["upcomingEvents.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoMA,OAAA,cAAA,MAAA,mCAAA;AACA,SAAA,SAAA,QAAA,cAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,cAAA,EAAA,cADA;AAEA,MAAA,UAAA,EAAA,EAFA;AAGA,MAAA,UAAA,EAAA,IAHA;AAIA,MAAA,QAAA,EAAA,KAJA;AAKA,MAAA,yBAAA,EAAA,KALA;AAMA,MAAA,QAAA,EAAA,EANA;AAOA,MAAA,UAAA,EAAA;AAPA,KAAA;AASA,GAXA;AAYA,EAA